@prefix bc: <http://knowledgelinks.io/ns/bibcat/> .
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix kds: <http://knowledgelinks.io/ns/data-structures/> .
@prefix kdr: <http://knowledgelinks.io/ns/data-resources/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pcdm: <http://pcdm.org/models#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix relators: <http://id.loc.gov/vocabulary/relators/> .
@prefix dcr: <http://knowledgelinks.io/ns/dc/> .
@prefix loc: <http://id.loc.gov/> .
@prefix schema: <http://schema.org/> .

bc:bf-Organization a kds:PropertyLinker;
    kds:destPropUri [ bf:heldBy <https://www.coloradocollege.edu/> ] ;
    kds:destClassUri bf:Item .

bc:mods-abstract a kds:PropertyLinker;
    kds:srcPropXpath "mods:abstract" ;
    kds:destPropUri rdf:value ;
    kds:destClassUri bf:Summary ;
    kds:linkedRange bf:summary ;
    kds:linkedClass bf:Instance .

bc:mods-dateCreated a kds:PropertyLinker;
    kds:srcPropXpath "mods:originInfo/mods:dateCreated" ;
    kds:destPropUri bf:originDate ;
    kds:destClassUri bf:Work ;
    kds:linkedRange bf:instanceOf ;
    kds:linkedClass bf:Instance .

bc:mods-Sponsor a kds:PropertyLinker;
    kds:srcPropXpath "mods:name/mods:role[mods:roleTerm='sponsor']/../mods:namePart";
    kds:destPropUri rdfs:label ;
    kds:destClassUri bf:Organization ;
    kds:destAdditionalPropUris [ bf:partOf <https://www.coloradocollege.edu/> ] ;
    kds:linkedRange relators:spn ;
    kds:linkedClass bf:Instance .

bc:mods-AudioWork a kds:PropertyLinker;
    kds:srcPropXpath "mods:subject[mods:genre='Audio recording']";
    kds:destPropUri rdf:type;
    kds:destClassUri  bf:Audio ;
    kds:linkedRange bf:instanceOf ;
    kds:linkedClass bf:Instance .
